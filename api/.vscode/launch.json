    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework WORKING",
      "args": ["${workspaceFolder}/src/main.ts"],
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register", "-r", "tsconfig-paths/register"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      // "restart": true,
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug with Auto-Restart (npm)",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start:debug:nodemon"
      ],
      "console": "integratedTerminal",
      "restart": true,
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceRoot}",
      "skipFiles": ["<node_internals>/**"],
      "autoAttachChildProcesses": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Standard (nest start --debug)",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start:debug"
      ],
      "console": "integratedTerminal",
      "restart": true,
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceRoot}",
      "skipFiles": ["<node_internals>/**"],
      "autoAttachChildProcesses": true
    }
